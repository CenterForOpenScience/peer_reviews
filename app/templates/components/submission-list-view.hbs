{{yield}}
  <div class="listViewHolder">
        <table id="submissions" class="table table-hover" align="center" >
            <thead>
            <tr>
                <th>
                    #
                </th>
                <th>
                    Title
                </th>
                <th>
                    Conference
                </th>
                <th>
                    Review Deadline
                </th>
                <th>
                    Status
                </th>
            </tr>
            </thead>
            <tbody class="searchable">
            {{#each model.reviewsdate as |d|}}

                <tr  {{action 'goToSubmission' d.id }}>
                    <td>
                      {{d.id}}
                    </td>
                    <td>
                      {{d.title}}
                    </td>
                    <td>
                      {{d.conference}}
                    </td>
                    <td>
                      {{d.reviewdeadline}}
                    </td>
                    <td class="st">
                      {{d.status}}
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>